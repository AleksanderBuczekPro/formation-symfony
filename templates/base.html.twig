<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>Intramilo - {% block title %}Welcome!{% endblock %}</title>

        <meta name="viewport" content="width=device-width, user-scalable=no">

        {# <link rel="stylesheet" href="{{ asset('build/css/app.css') }}"> #}
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.2/css/all.css" integrity="sha256-piqEf7Ap7CMps8krDQsSOTZgF+MU/0MPyPW2enj5I40=" crossorigin="anonymous" />
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/weather-icons/2.0.9/css/weather-icons.min.css" integrity="sha256-KcCcakqMaamBrTFaxb9tkrP2iq1X8vUnsm86W8pRcgI=" crossorigin="anonymous" />
        {# <link rel="stylesheet" href="/css/app.css"> #}
        <link rel="stylesheet" href="https://unpkg.com/shards-ui@latest/dist/css/shards.min.css">

        <link rel="stylesheet" href="/css/sidebar.css">

        <link rel="stylesheet" href="/css/main.css">
        <link rel="stylesheet" href="/css/files.css">
        <link rel="stylesheet" href="/css/sheet.css">
        <link rel="stylesheet" href="/css/documentation.css">
        <link rel="stylesheet" href="/css/admin.css">
        <link rel="stylesheet" href="/css/header.css">

        <link rel="stylesheet" href="/css/login.css">
        <link rel="stylesheet" href="/css/util.css">

        {# Fonts #}
        <link href="https://fonts.googleapis.com/css?family=Fira+Sans300,800&display=swap" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Karla&display=swap" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Fira+Mono&display=swap" rel="stylesheet">


        

        {{ encore_entry_link_tags('css/app') }}
        {% block stylesheets %}{% endblock %}
    </head>
    <body>

    {% if app.request.attributes.get('_route') != 'account_login' %}
    {% include 'partials/header.html.twig'  %}
    {% endif %}
    
    {% block body %}{% endblock %}

    {% if app.request.attributes.get('_route') != 'account_login' %}
    {% include 'partials/footer.html.twig'  %}
    {% endif %}
    

        {{ encore_entry_script_tags('js/app') }}
        {% block javascripts %}
        
            <script src="{{ asset('build/css/app.css') }}"></script>
            
            <!-- Bootstrap core JavaScript -->
            <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
            <script src="https://unpkg.com/shards-ui@latest/dist/js/shards.min.js"></script>

        

                  
        
        
        {% endblock %}
    </body>
</html>
